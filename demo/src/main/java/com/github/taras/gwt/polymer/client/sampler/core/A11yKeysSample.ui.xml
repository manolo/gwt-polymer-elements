<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:p='urn:import:com.github.taras.gwt.polymer.client.widget'>
    
    <g:HTMLPanel>
        <span>Press any of these keys: <em ui:field="label">{{keys}}</em></span>
        <p:CoreA11yKeys ui:field="a11y"/>
